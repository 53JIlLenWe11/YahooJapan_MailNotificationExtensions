{
	"name": "Yahoo! Japanメール通知",
	"version": "0.4",
	"manifest_version": 2,
	"description": "Yahoo! Japanメール通知拡張 30秒に一度自動で確認 ログインしておく必要があります。",
	"permissions": [
		"https://www.yahoo.co.jp/*",
		"https://jp.mg5.mail.yahoo.co.jp/"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Yahoo! Japanメール通知"
	},
	"background": {
		"scripts": [
			"jquery-3.2.0.min.js",
			"background.js"
		]
	}
}
